<nav>
  <%= render partial: "render/nav1" %>
</nav>


<main class="MakeStandard-screen wf-roundedmplus1c">
  <article>
    <section id="BGP-researchable" style="">
      <div class="BGP-Opacity1" style="padding: 125px 0px 0px;">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-0"></div>
            <div class="col-11">
              <div class="col-md-8">
                <h1 class="mt-4 mb-5"><img class="icon-blank" src="/assets/Admin/file.png"><strong>テンプレートプロトコル作成</strong></h1>
              </div>
            </div>
          <div class="col-0"></div>
            <div class="col-10">
              <form>
                <div class="form-group mb-5">
                  <label class="col-md-8 col-form-label mb-2" for="B_Name">タイトル（プロトコルの正式名称）</label>
                  <div class="col-md-7">
                    <input type="text" class="form-control" id="B_Name">
                  </div>
                </div>

                <div class="form-group mb-5">
                  <label class="col-md-8 col-form-label mb-2" for="B_Name">プロトコル作成日</label>
                  <div class="col-md-5">
                    <a id="year"></a><a id="month"></a><a id="day"></a>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
        <div class="container">        <!-- 全体を囲むコンテナ -->
          <div class="row justify-content-center">
          <div class="col-0"></div>
            <div class="col-10">
              <form>
               <div class="form-group mb-5">
                  <label class="col-md-3 col-form-label" for="B_Name">実験方法</label>
                  <div class="col-md-9" style="max-width: 100%;">
                    <textarea rows="7" class="form-control" id="Q_Ask"></textarea>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>                <!-- 全体を囲むコンテナ -->
        <div class="container">
          <div class="row justify-content-center">
            <div class="col-0"></div>
              <div class="col-10">
                <form>
                  <div class="form-group col-md-8 mb-4">
                    <label for="receipt">登録するカテゴリー（選択もしくは作成）</label>
                    <select class="custom-select" id="receipt">
                      <option>選択</option>
                      <option>hoge1</option>
                      <option>hoge2</option>
                      <option>hoge3</option>
                    </select>
                  </div>

                  <div class="form-group mb-5">
                  <div class="col-md-8">
                    <input type="text" class="form-control" id="B_Name">
                  </div>
                </div>

                  <button class="btn btn-outline-primary" style="margin: 0px auto 75px; display: block;">登録
                  </button>
              </form>
            </div>
          </div>
        </div>
      </div>                <!-- 全体を囲むコンテナ -->
      </div>
    </section>
  </article>
</main>


